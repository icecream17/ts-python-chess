{
   "name": "ts-python-chess",
   "version": "0.1.2",
   "description": "Yet another chess package. python-chess, but in javascript",
   "main": "src/chess.js",
   "type": "module",
   "scripts": {
      "build": "npm install --include=dev",
      "test": "npm run test-functionality && npm run test-syntax",
      "test-functionality": "npx jest",
      "test-syntax": "npx eslint src/*.ts tests/*.test.ts && node success.js",
      "upload-coverage": "npx jest --ci --coverage && npx codecov"
   },
   "keywords": [
      "chess",
      "python-chess"
   ],
   "author": "icecream17",
   "license": "GPL-3.0-or-later",
   "dependencies": {
      "@babel/core": "^7.16.0",
      "@babel/preset-env": "^7.16.4",
      "@babel/preset-typescript": "^7.16.0",
      "@types/jest": "^27.0.3",
      "@typescript-eslint/eslint-plugin": "^4.33.0",
      "@typescript-eslint/parser": "^4.33.0",
      "babel-jest": "^27.4.2",
      "eslint": "^7.32.0",
      "eslint-config-standard-with-typescript": "^21.0.1",
      "eslint-plugin-import": "^2.25.3",
      "eslint-plugin-jest": "^25.3.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^5.2.0",
      "jest": "^27.4.2",
      "ts-node": "^10.4.0",
      "typescript": "^4.5.2"
   },
   "devDependencies": {
      "chalk": "^4.1.2",
      "codecov": "^3.8.3"
   }
}
