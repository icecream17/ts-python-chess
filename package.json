{
   "name": "ts-python-chess",
   "version": "0.1.1",
   "description": "Yet another chess package. python-chess, but in javascript",
   "main": "src/chess.js",
   "scripts": {
      "build": "npm install --include=dev",
      "test": "npm run test-functionality && npm run test-syntax",
      "test-functionality": "npx jest",
      "test-syntax": "npx eslint src/*.ts tests/*.test.ts && node success.js",
      "upload-coverage": "npx jest --ci --coverage && npx codecov"
   },
   "keywords": [
      "chess",
      "python-chess"
   ],
   "author": "icecream17",
   "license": "GPL-3.0-or-later",
   "dependencies": {
      "@babel/core": "^7.14.8",
      "@babel/preset-env": "^7.14.8",
      "@babel/preset-typescript": "^7.14.5",
      "@types/jest": "^26.0.24",
      "@typescript-eslint/eslint-plugin": "^4.28.5",
      "@typescript-eslint/parser": "^4.28.5",
      "babel-jest": "^27.0.5",
      "eslint": "^7.31.0",
      "eslint-config-standard-with-typescript": "^20.0.0",
      "eslint-plugin-import": "^2.23.4",
      "eslint-plugin-jest": "^24.4.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^5.1.0",
      "jest": "^27.0.6",
      "ts-node": "^10.1.0",
      "typescript": "^4.3.5"
   },
   "devDependencies": {
      "chalk": "^4.1.1",
      "codecov": "^3.8.3"
   }
}
