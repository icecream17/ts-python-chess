{
   "name": "ts-python-chess",
   "version": "0.1.0",
   "description": "Yet another chess package. python-chess, but in javascript",
   "main": "src/chess.js",
   "scripts": {
      "build": "npm install --include=dev",
      "test": "npm run test-functionality && npm run test-syntax",
      "test-functionality": "npx jest",
      "test-syntax": "npx eslint src/*.ts tests/*.test.ts && node success.js",
      "upload-coverage": "npx jest --ci --coverage && npx codecov"
   },
   "keywords": [
      "chess",
      "python-chess"
   ],
   "author": "icecream17",
   "license": "GPL-3.0-or-later",
   "dependencies": {
      "@types/jest": "^26.0.20",
      "@typescript-eslint/eslint-plugin": "^4.16.1",
      "@typescript-eslint/parser": "^4.16.1",
      "eslint": "^7.21.0",
      "eslint-config-standard-with-typescript": "^20.0.0",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-jest": "^24.1.3",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^4.3.1",
      "jest": "^26.6.3",
      "ts-jest": "^26.5.3",
      "ts-node": "^9.1.1",
      "typescript": "^4.2.3"
   },
   "devDependencies": {
      "chalk": "^4.1.0",
      "codecov": "^3.8.1"
   }
}
