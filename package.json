{
   "name": "ts-python-chess",
   "version": "0.1.2",
   "description": "Yet another chess package. python-chess, but in javascript",
   "main": "src/chess.js",
   "scripts": {
      "build": "npm install --include=dev",
      "test": "npm run test-functionality && npm run test-syntax",
      "test-functionality": "npx jest",
      "test-syntax": "npx eslint src/*.ts tests/*.test.ts && node success.mjs",
      "upload-coverage": "npx jest --ci --coverage && npx codecov"
   },
   "keywords": [
      "chess",
      "python-chess"
   ],
   "author": "icecream17",
   "license": "GPL-3.0-or-later",
   "dependencies": {
      "@babel/core": "^7.16.12",
      "@babel/preset-env": "^7.16.11",
      "@babel/preset-typescript": "^7.16.7",
      "@types/jest": "^29.0.0",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "babel-jest": "^29.0.0",
      "eslint": "^8.0.0",
      "eslint-config-standard-with-typescript": "^22.0.0",
      "eslint-plugin-import": "^2.25.4",
      "eslint-plugin-jest": "^26.0.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^6.0.0",
      "jest": "^29.0.0",
      "ts-node": "^10.4.0",
      "typescript": "^4.5.5"
   },
   "devDependencies": {
      "chalk": "^5.0.0",
      "codecov": "^3.8.3"
   }
}
